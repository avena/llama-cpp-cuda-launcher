    "editor.tabSize": 2,
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "python.defaultInterpreterPath": "[MISSING: PYTHON_DEFAULT_INTERPRETER]",
