{
  "workbench.iconTheme": "vscode-icons",
  "window.zoomLevel": 2,
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 17,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "editor.renderWhitespace": "all",
  "editor.fontLigatures": true,
  "editor.inlineSuggest.enabled": true,
